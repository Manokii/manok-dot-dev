{
  "name": "manok-dot-dev",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "init:env": "sh ./init-env.sh",
    "preview": "next build && next start",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "db:check": "drizzle-kit check:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:pull": "drizzle-kit introspect:pg",
    "db:migrate": "pnpm dlx tsx ./db/migrate",
    "db:studio": "drizzle-kit studio --port 1337 --verbose",
    "db:seed": "pnpm dlx tsx ./db/seed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.1",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-avatar": "1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "2.0.5",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@t3-oss/env-nextjs": "0.6.0",
    "@tabler/icons-react": "2.25.0",
    "@uploadthing/react": "5.1.0",
    "class-variance-authority": "0.6.1",
    "clsx": "1.2.1",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "16.3.1",
    "drizzle-orm": "0.27.2",
    "drizzle-zod": "0.4.4",
    "next": "13.4.9",
    "next-auth": "4.22.1",
    "next-themes": "0.2.1",
    "postgres": "3.3.5",
    "react": "18.2.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-hook-form": "7.45.1",
    "react-markdown": "8.0.7",
    "remark-gfm": "3.0.1",
    "server-only": "0.0.1",
    "tailwind-merge": "1.13.2",
    "tailwindcss-animate": "1.0.6",
    "uploadthing": "5.1.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.0.2",
    "@types/node": "20.4.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "10.4.14",
    "drizzle-kit": "0.19.5",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-tailwindcss": "3.13.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.25",
    "prettier": "3.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "**": "prettier --write --ignore-unknown"
  }
}
